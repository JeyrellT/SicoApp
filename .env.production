# Configuración de Producción
# Optimizaciones para build de GitHub Pages

# Desactivar source maps en producción (reduce 30-40% el tamaño)
GENERATE_SOURCEMAP=false

# No inline runtime chunk (mejor para caching)
INLINE_RUNTIME_CHUNK=false

# Configuración de entorno
REACT_APP_ENV=production

# Desactivar profiler de React en producción
REACT_APP_PROFILER=false

# Configuración de PWA
REACT_APP_PWA_ENABLED=true

# URL base para GitHub Pages
PUBLIC_URL=https://jeyrellt.github.io/SicoApp
